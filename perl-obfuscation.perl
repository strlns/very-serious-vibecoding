#!/usr/bin/env perl
use POSIX qw(atan sin cos abs);
use constant PI => 3.14159265358979323846;

sub D{my($k,$l,$v,$N)=@_;
    join('',map{
        chr((int( abs( atan( ($_+ $k) / ($l * (sin($_) + cos($_))) ) * $v * $_ ) ) % 95) + 32)
    } 1..$N)
}

sub S{
    my @T = @_;
    join('', map { chr($_) } @T);
}

