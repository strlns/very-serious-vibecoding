sub recover_params {
    my ($Px, $Py, $N) = @_;
    my $v = $Py / $N;
    my $l = ($Px - $EC_A) / ($EC_B + 10);
    my $k = $l + 0.51;
    return (sprintf("%.2f", $k), sprintf("%.1f", $l), $v, $N);
}

